<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:rgc="http://common.regurgitator.emarte.com"
           xmlns:rgw="http://web.extensions.regurgitator.emarte.com"
           targetNamespace="http://web.extensions.regurgitator.emarte.com" elementFormDefault="qualified">

    <xs:import schemaLocation="regurgitatorCommon.xsd" namespace="http://common.regurgitator.emarte.com"/>

    <xs:complexType name="process-response">
        <xs:sequence>
            <xs:any minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
	</xs:complexType>

    <xs:complexType name="http-call">
        <xs:sequence>
            <xs:element name="process-response" type="rgw:process-response" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="host" type="rgc:populated-string" use="required"/>
		<xs:attribute name="port" type="xs:integer"  use="required"/>
        <xs:attribute name="username" type="rgc:populated-string"/>
		<xs:attribute name="password" type="rgc:populated-string"/>
		<xs:attribute name="id" type="rgc:populated-string"/>
	</xs:complexType>

	<xs:complexType name="query-param-processor">
		<xs:attribute name="key" type="rgc:populated-string" use="required"/>
	</xs:complexType>

	<xs:element name="http-call" type="rgw:http-call"/>
	<xs:element name="query-param-processor" type="rgw:query-param-processor"/>
</xs:schema>
